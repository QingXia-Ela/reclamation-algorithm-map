{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build:desktop",
    "beforeDevCommand": "npm run dev:tauri",
    "frontendDist": "../dist-desktop",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "targets": [
      "app"
    ],
    "resources": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "生息演算网页地图桌面版",
  "mainBinaryName": "生息演算网页地图桌面版",
  "version": "0.1.0",
  "identifier": "com.arknight.ra.taleswithinthesand.map",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "生息演算网页地图离线版",
        "width": 1280
      }
    ],
    "security": {
      "csp": null
    }
  }
